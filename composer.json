{
    "name": "jamarcer/doctrine-dbal-apm",
    "description": "Elastic APM for Doctrine DBAL",
    "version": "2.0.0",
    "license": "MIT",
    "type": "library",
    "authors": [
    {
        "name": "Jesús Martínez",
        "email": "jamarcer@gmail.com"
    }
    ],
    "autoload": {
        "psr-4": {
            "Jamarcer\\APM\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
          "Jamarcer\\Tests\\APM\\": "tests/"
        }
    },
    "require": {
        "php": "^8.0",
        "doctrine/dbal": "^2.13",
        "zoilomora/elastic-apm-agent-php": "^1.0",
        "aaronidas/apm-sql-lexer": "^0.0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.2.*"
        }
    }
}
